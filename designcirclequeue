class MyCircularQueue {
public:
    MyCircularQueue(int k) { // intialize our variables
        cap = k;
        head = tail = count = 0;
        data = new int[k];
    }
    
    bool enQueue(int value) {
        if (isFull()) return false; // if its full cannot add to queue
        data[tail] = value;
        tail += 1;
        tail = tail % cap; // loop back to use free slots
        count += 1;
        return true;
    }
    
    bool deQueue() {
        if (isEmpty()) return false; //  if its empty cannot remove
        head += 1;
        head = head % cap;
        count -= 1;
        return true;
    }
    
    int Front() {
        if (isEmpty()){
            return -1;
        }
        return data[head];
    }
    
    int Rear() {
        if (isEmpty()) return -1;
        return data[(tail + cap - 1) % cap];
    }
    
    bool isEmpty() {
        if (count == 0) return true; // if the count is 0 then its empty
        else return false;
    }
    
    bool isFull() {
        if (count == cap) return true; // if the count is == to the cap then we are full
        else return false;
    }
private:
int cap;
int head;
int tail;
int count;
int *data;
};

/**
 * Your MyCircularQueue object will be instantiated and called as such:
 * MyCircularQueue* obj = new MyCircularQueue(k);
 * bool param_1 = obj->enQueue(value);
 * bool param_2 = obj->deQueue();
 * int param_3 = obj->Front();
 * int param_4 = obj->Rear();
 * bool param_5 = obj->isEmpty();
 * bool param_6 = obj->isFull();
 */
